{
  "name": "react-startup",
  "version": "0.0.1",
  "description": "the startup base on react and restify",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint ./src/**/*.js",
    "lint:tests": "eslint ./tests/**/*.spec.js",
    "compile": "set NODE_ENV=production&& webpack --progress --colors",
    "compile-mac": "NODE_ENV=production webpack --progress --colors",
    "start": "set NODE_ENV=development&& npm run dev",
    "start-mac": "NODE_ENV=development npm run dev",
    "dev": "webpack-dev-server --hot --inline --progress --colors --quiet false --noInfo false --port 3000",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "node ./node_modules/karma/bin/karma start",
    "test:dev": "npm run test -- --watch",
    "deploy": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rackt/redux.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rackt/redux/issues"
  },
  "homepage": "http://rackt.github.io/redux",
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "classnames": "^2.2.1",
    "history": "^1.17.0",
    "humps": "^0.6.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.1.1",
    "lodash": "^3.10.1",
    "moment": "^2.11.2",
    "react": "^0.14.3",
    "react-date-range": "^0.2.3",
    "react-dom": "^0.14.3",
    "react-paginate": "^0.5.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "react-select": "^0.9.1",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.0",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.0.2",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chalk": "^1.1.1",
    "concurrently": "^0.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.6.4",
    "moment": "^2.11.2",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
